#: Flight_Freedom/scenarios/02b_Swamps_Jamolar.cfg:4
msgid "Swamps of the Jamolar"
msgstr ""

#. [scenario]: id=Swamps_Jamolar
#: Flight_Freedom/scenarios/02b_Swamps_Jamolar.cfg:22
msgid "Survive for 20 turns"
msgstr ""

