msgstr ""

#. [message]: speaker=unit
#: Flight_Freedom/scenarios/19_Liberation.cfg:1275
msgid "The bridge is almost finished -- I could almost jump across."
msgstr ""

#. [message]: speaker=unit
#: Flight_Freedom/scenarios/19_Liberation.cfg:1311
msgid "The bridge is complete!"
msgstr ""

#. [message]: speaker=Cicyn
#: Flight_Freedom/scenarios/19_Liberation.cfg:1366
